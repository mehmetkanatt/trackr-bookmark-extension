{
  "manifest_version": 2,
  "name": "Trackr",
  "version": "1.0",
  "permissions": ["contextMenus", "activeTab", "webRequest", "webRequestBlocking", "<all_urls>"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "favicon.ico"
  }
}
